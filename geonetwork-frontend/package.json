{
  "name": "geonetwork",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build-all": "ng build gapi && ng build g5api && ng build glib && ng build gapp && ng build gwebcomponents --output-hashing none --base-href /webcomponents/",
    "build-lib-gapi": "ng build gapi",
    "build-lib-g5api": "ng build g5api",
    "build-lib-glib": "ng build glib",
    "build-lib-gwebcomponents": "ng build gwebcomponents --output-hashing none",
    "build-gapp": "ng build gapp",
    "start": "ng serve --project=gapp",
    "start-webcomponents": "ng serve --project=gwebcomponents",
    "watch-lib-glib": "ng build glib --watch",
    "watch-lib-gapi": "ng build gapi --watch",
    "watch-lib-g5api": "ng build g5api --watch",
    "prettier:check": "prettier --check \"**/*.{ts,html,scss,css}\"",
    "prettier": "prettier --write \"**/*.{ts,html,scss,css,json}\"",
    "test": "ng test glib",
    "openapi-generator": "openapi-generator-cli generate -i projects/gapi/gapi.json  -g typescript-fetch -o  projects/gapi/src/lib  --skip-validate-spec",
    "openapi-generator-g5api": "openapi-generator-cli generate -i projects/g5api/g5api.json  -g typescript-fetch -o  projects/g5api/src/lib  --skip-validate-spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19",
    "@angular/common": "^19",
    "@angular/compiler": "^19",
    "@angular/core": "^19",
    "@angular/elements": "^19",
    "@angular/forms": "^19",
    "@angular/platform-browser": "^19",
    "@angular/platform-browser-dynamic": "^19",
    "@angular/router": "^19",
    "@elastic/elasticsearch": "^8.14.0",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@ngrx/operators": "^19.0.0",
    "@ngrx/signals": "^19.0.0",
    "@primeng/themes": "^19.0.6",
    "primeng": "^19.0.6",
    "jsonpath-plus": "^10.2.0",
    "ngx-ace-wrapper": "^17.0.0",
    "papaparse": "^5.4.1",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.3.4",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19",
    "@angular/cli": "^19",
    "@angular/compiler-cli": "^19",
    "@cypress/schematic": "^2.5.2",
    "@eslint/js": "^9.6.0",
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "@types/jasmine": "~5.1.0",
    "@types/papaparse": "^5.3.14",
    "angular-eslint": "18.2.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.1",
    "globals": "^15.8.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.0.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~5.6.3"
  }
}
